Cell["\<\
This code is a pretty simple extension of the stuff above, but demonstrates \
the added complexity from solving higher-order equations using elimination.\
\>", "Text",
 CellChangeTimes->{{3.668625311486361*^9, 3.668625342023047*^9}}],

Cell[BoxData[{
 RowBox[{"Clear", "@", "elim3"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"elim3", "[", 
   RowBox[{"eq1_", ",", "eq2_", ",", "eq3_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"soleq1", "=", 
      RowBox[{
       RowBox[{"Solve", "[", 
        RowBox[{"eq1", ",", "x"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"soleq2", "=", 
      RowBox[{
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"eq2", "/.", "soleq1"}], ",", "y"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"zval", "=", 
      RowBox[{"z", "/.", 
       RowBox[{
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{"eq3", "/.", 
           RowBox[{"Join", "[", 
            RowBox[{"soleq1", ",", "soleq2"}], "]"}]}], ",", "z"}], "]"}], 
        "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yval", "=", 
      RowBox[{
       RowBox[{"soleq2", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}], "/.", 
       RowBox[{"z", "\[Rule]", "zval"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"xval", "=", 
      RowBox[{
       RowBox[{"soleq1", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "2"}], "]"}], "]"}], "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"z", "\[Rule]", "zval"}], ",", 
         RowBox[{"y", "\[Rule]", "yval"}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
